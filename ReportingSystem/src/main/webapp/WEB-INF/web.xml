<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>registerEmployee</servlet-name>
  	<display-name>registerEmployee</display-name>
  	<description></description>
  	<servlet-class>com.Reports.web.Controllers.registerEmployee</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loginEmployee</servlet-name>
  	<display-name>loginEmployee</display-name>
  	<description></description>
  	<servlet-class>com.Reports.web.Controllers.loginEmployee</servlet-class>
  </servlet>
  
   <servlet>
  	<servlet-name>viewIncidents</servlet-name>
  	<display-name>viewIncidents</display-name>
  	<description></description>
  	<servlet-class>com.Reports.web.Controllers.viewIncidents</servlet-class>
  </servlet> 
 
  <servlet-mapping>
  	<servlet-name>registerEmployee</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loginEmployee</servlet-name>
  	<url-pattern>/loginEmployee</url-pattern>
  </servlet-mapping>
 <servlet-mapping>
  	<servlet-name>viewIncidents</servlet-name>
  	<url-pattern>/viewIncidents</url-pattern>
  </servlet-mapping> 
</web-app>
